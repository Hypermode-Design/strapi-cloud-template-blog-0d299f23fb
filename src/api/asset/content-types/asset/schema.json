{
  "kind": "collectionType",
  "collectionName": "assets",
  "info": {
    "singularName": "asset",
    "pluralName": "assets",
    "displayName": "Asset",
    "description": "Font assets like images, files, and media"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "asset_url": {
      "type": "string",
      "required": true
    },
    "asset_type": {
      "type": "enumeration",
      "default": "image",
      "enum": [
        "image",
        "file",
        "video",
        "demo"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "sort_order": {
      "type": "integer",
      "default": 0
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    }
  }
}
